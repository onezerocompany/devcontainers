{
  "name": "devcontainers",
  "image": "ghcr.io/onezerocompany/devcontainer:base",
  "features": {
    "ghcr.io/onezerocompany/devcontainers/features/common-utils": {},
    "ghcr.io/onezerocompany/devcontainers/features/mise": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers"
      ]
