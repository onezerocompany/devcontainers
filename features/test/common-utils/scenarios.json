{
  "default": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {}
    }
  },
  "bash-shell": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "defaultShell": "bash"
      }
    }
  },
  "minimal": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "starship": false,
        "zoxide": false,
        "eza": false,
        "webDev": false,
        "utilities": false
      }
    }
  },
  "no-root-config": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "configureForRoot": false
      }
    }
  },
  "kubernetes-bundle": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "kubernetes": true
      }
    }
  },
  "bundles-only": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "starship": false,
        "zoxide": false,
        "eza": false,
        "bat": false,
        "webDev": true,
        "networking": true,
        "utilities": true
      }
    }
  },
  "security-focused": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "buildTools": false,
        "networking": true
      }
    }
  },
  "custom-motd": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "motdLogo": "dev",
        "motdInstructions": "Run 'npm start' to begin development\nUse 'npm test' to run tests",
        "motdNotice": "Development environment - not for production use",
        "shims": false,
        "completions": false
      }
    }
  },
  "docker-motd": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "motdLogo": "docker",
        "motdInstructions": "Docker commands available: docker, docker-compose"
      }
    }
  },
  "no-motd": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "motd": false
      }
    }
  },
  "config-system": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {}
    }
  },
  "minimal-config": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {
        "starship": false,
        "zoxide": false,
        "eza": false,
        "webDev": false,
        "utilities": false
      }
    }
  },
  "base-image-compat": {
    "image": "ghcr.io/onezerocompany/base:latest",
    "features": {
      "common-utils": {}
    }
  }
}